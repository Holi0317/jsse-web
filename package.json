{
  "name": "jsse-web",
  "version": "1.0.0",
  "description": "Web frontend for jsse",
  "main": "dist/main.js",
  "scripts": {
    "lint": "npm run lint:js && npm run lint:ts",
    "lint:js": "eslint --ext .jsx,.js --cache src webpack-cfg webpack.config.js",
    "lint:ts": "tslint src",
    "clean": "del-cli dist",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean",
    "build:dev": "cross-env NODE_ENV=development npm run webpack -- --progress --profile",
    "prebuild:prod": "npm run clean",
    "build:prod": "cross-env NODE_ENV=production npm run webpack -- --progress --profile",
    "build-splash-css": "postcss splash.css --no-map -u cssnano postcss-cssnext",
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "cross-env NODE_ENV=development npm run webpack-dev-server -- --inline --progress --profile --watch",
    "server:prod": "serve -s -p 8080 dist",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "ci": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Holi0317/jsse-web.git"
  },
  "author": "Holi0317 <holliswuhollis@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Holi0317/jsse-web/issues"
  },
  "homepage": "https://github.com/Holi0317/jsse-web#readme",
  "devDependencies": {
    "@types/lodash-es": "4.14.6",
    "@types/material-ui": "0.18.0",
    "@types/node": "8.0.25",
    "@types/react": "16.0.5",
    "@types/react-datepicker": "0.52.1",
    "@types/react-dom": "15.5.4",
    "@types/react-redux": "5.0.5",
    "ajv": "5.2.2",
    "awesome-typescript-loader": "3.2.3",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "5.0.5",
    "css-loader": "0.28.5",
    "del-cli": "1.1.0",
    "eslint": "4.5.0",
    "extract-text-webpack-plugin": "3.0.0",
    "html-webpack-plugin": "2.30.1",
    "postcss-cli": "4.1.0",
    "postcss-import": "10.0.0",
    "postcss-loader": "2.0.6",
    "postcss-nesting": "4.1.0",
    "postcss-reporter": "5.0.0",
    "serve": "6.0.6",
    "serviceworker-webpack-plugin": "0.2.3",
    "style-loader": "0.18.2",
    "tslint": "5.7.0",
    "typescript": "2.4.2",
    "uglify-es": "3.0.28",
    "uglifyjs-webpack-plugin": "1.0.0-beta.1",
    "webpack": "3.5.5",
    "webpack-bundle-analyzer": "2.9.0",
    "webpack-dev-server": "2.7.1",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "4.1.0"
  },
  "dependencies": {
    "firebase": "4.3.0",
    "lodash-es": "4.17.4",
    "material-ui": "0.19.0",
    "moment": "2.18.1",
    "postcss": "6.0.9",
    "react": "16.0.0-beta.5",
    "react-datepicker": "0.53.0",
    "react-dom": "16.0.0-beta.5",
    "react-redux": "5.0.6",
    "react-redux-firebase": "2.0.0-beta.7",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "reselect": "3.0.1"
  }
}
